#include<stdio.h>
int main(){
    int num;
    scanf("%d", &num);
    int len=0;
    int t=num;
    while(t){
        t/=16;
        len++;
    }
    char hex[len+3];
    hex[0]='0';
    hex[1]='x';
    hex[len+2]=0;
    for(int i=0; i<len; i++){
        int res=num%16;
        if(res<=9)hex[len+1-i]=res+'0';
        else hex[len+1-i]=res-10+'A';
        num/=16;
    }
    printf("%s\n", hex);
    return 0;
}
